{{~it :player}}
  <li data-id="{{=player.id}}" class="leader-board column-12">
    <img class="player-gravatar left" src="{{=player.gravatar}}" alt="{{=player.nickname}}">
    <h3 class="player-nickname">{{=player.nickname}}</h3><h3 class="player-nickname right">{{=player.skulls}}</h3>
  </li>
{{~}}
